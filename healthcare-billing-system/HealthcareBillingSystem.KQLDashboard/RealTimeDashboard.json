  "queries": [
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// I want \"Department\", \"department_code\", \"billing_codes\", \"base_claims\", \"Billed_Amount\", \"Collection_Amount\", \"Claims_Rejections\" data within the sensor_data column on the bronze_contoso_health table to be parsed and separated into their own column on a new table titled  silver_contoso_health. Add \"timestamp\", \"event_id\", \"source\".  Then I want to have another column for the percentage Collection_Amount over Billed_Amount with a percentage sign after the value.\n//\nbronze_contoso_health\n| extend Department = tostring(sensor_data.Department),\n         department_code = tostring(sensor_data.department_code),\n         billing_codes = sensor_data.billing_codes,\n         base_claims = toint(sensor_data.base_claims),\n         Billed_Amount = toreal(sensor_data.Billed_Amount),\n         Collection_Amount = toreal(sensor_data.Collection_Amount),\n         Claims_Rejections = toint(sensor_data.Claims_Rejections)\n| extend Collection_Percentage = strcat(tostring(round(Collection_Amount / Billed_Amount * 100, 2)), \"%\")\n| project source, Billed_Amount, Collection_Amount, Collection_Percentage",
      "id": "2fc0a962-9710-4a07-9853-25c80f87ea07",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// I want to write  kql query to show all source categories per hour and claims rejections.\n//\nsilver_contoso_health\n| summarize claimsRejections=sum(Claims_Rejections) by source, hour=bin(timestamp, 1d)",
      "id": "18b805ff-49e8-4d94-b30a-3acac37fabe3",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// remove timestamp and make it a daily intervals and show the difference between each day on a separate column\n//\nsilver_contoso_health\n| summarize Inpatient=sum(Claims_Rejections), OutPatient=sum(Billed_Amount) - 62 by Department = department, day=bin(timestamp, 1d)\n| sort by Department, day asc\n| extend prevInpatient=prev(Inpatient, 1)\n| extend InpatientDailyDifference=Inpatient - prevInpatient\n| project Department, Inpatient, OutPatient, InpatientDailyDifference",
      "id": "3a02648a-1917-4e2b-bf09-b73e3231af3e",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// add a dollar sign in front of it\n//\nsilver_contoso_health\n| summarize avgBilledAmount=toint(avg(Billed_Amount)*10) by bin(timestamp, 1d)\n| extend avgBilledAmount=strcat(\"$\", tostring(avgBilledAmount))",
      "id": "8b9ffda8-cbd6-4a3f-9c92-ef0417b648ae",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// add dollar sign in front\n//\nsilver_contoso_health\n| summarize dailyTotalResult=sum((Billed_Amount - Claims_Rejections) * 10) by bin(timestamp, 1d)\n| extend dailyTotalResultFormatted = strcat(\"$\", tostring(dailyTotalResult))",
      "id": "137ffb87-3507-438e-8754-1c911563d564",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// make it 10 minute intervals\n//\nsilver_contoso_health\n| summarize totalBilledAmount=sum(Billed_Amount), totalClaimsRejections=sum(Claims_Rejections) by bin(timestamp, 10m)\n| order by timestamp asc",
      "id": "ac59fbd4-9ffa-4913-a2e3-388b9c70bfb4",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// write a kql query to show billed amount minus collection amount by summed source\n//\nsilver_contoso_health\n| summarize totalBilledAmount=sum(Billed_Amount), totalCollectionAmount=sum(Collection_Amount) by source\n| project source, billedMinusCollection = totalBilledAmount - totalCollectionAmount",
      "id": "0820c8c6-c443-4092-bcd8-dd3b103b12e0",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8fc5432e-172f-43d8-b860-2c1ecf0d8076"
      },
      "text": "// This KQL query was generated by AI:\n// write a kql query to show billed amount minus collection amount by summed source\n//\nsilver_contoso_health\n| summarize totalBilledAmount=sum(Billed_Amount), totalCollectionAmount=sum(Collection_Amount) by source\n| project source, billedMinusCollection = totalBilledAmount - totalCollectionAmount",
      "id": "bd799774-fd4c-4581-b0e4-d8dc87e0c971",
